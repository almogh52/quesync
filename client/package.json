{
  "name": "client",
  "version": "1.0.0",
  "homepage": "./",
  "main": "public/electron.js",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^4.0.6",
    "electron-builder": "^20.38.5",
    "react-scripts": "^2.1.5",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "electron-is-dev": "^1.0.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "rmwc": "^4.0.6"
  },
  "scripts": {
    "react-start": "cross-env SASS_PATH=./node_modules react-scripts start",
    "react-build": "react-scripts build",
    "dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "electron .",
    "electron-pack": "build",
    "build": "npm run react-build && npm run electron-pack"
  },
  "build": {
    "appId": "com.almoghamdani.quesync",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
