{
  "name": "client",
  "version": "1.0.0",
  "homepage": "./",
  "main": "public/electron.js",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.2"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "dev": "concurrently \"set BROWSER=none&& npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build",
    "build": "npm run react-build && npm run electron-pack"
  },
  "build": {
    "appId": "com.almoghamdani.quesync",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
