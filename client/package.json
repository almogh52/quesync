{
	"name": "quesync",
	"version": "0.1.0",
	"author": "Almog Hamdani",
	"main": "public/electron.js",
	"homepage": "./",
	"devDependencies": {
		"concurrently": "^4.1.0",
		"cross-env": "^5.2.0",
		"electron": "^5.0.7",
		"electron-builder": "20.41.0",
		"electron-devtools-installer": "^2.2.4",
		"react-scripts": "^2.1.8",
		"redux-logger": "^3.0.6",
		"wait-on": "^3.2.0"
	},
	"dependencies": {
		"animejs": "^3.0.1",
		"chart.js": "^2.8.0",
		"electron-is-dev": "^1.1.0",
		"emoji-regex": "^8.0.0",
		"moment": "^2.24.0",
		"node-sass": "^4.12.0",
		"query-string": "^6.8.2",
		"react": "^16.8.6",
		"react-chartjs-2": "^2.7.6",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^16.8.6",
		"react-moment": "^0.9.2",
		"react-particles-js": "^2.6.0",
		"react-redux": "^6.0.1",
		"react-transition-group": "^4.1.1",
		"react-twemoji": "^0.2.3",
		"redux": "^4.0.1",
		"redux-persist": "^5.10.0",
		"redux-promise-middleware": "^6.1.1",
		"rmwc": "^5.5.2",
		"zxcvbn": "^4.4.2"
	},
	"scripts": {
		"react-start": "cross-env SASS_PATH=./node_modules react-scripts start",
		"react-build": "cross-env SASS_PATH=./node_modules react-scripts build",
		"dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
		"electron": "electron .",
		"electron-pack": "build",
		"backend": "npm run --prefix backend build",
		"prebuild": "npm run backend",
		"build": "npm run react-build && npm run electron-pack"
	},
	"build": {
		"appId": "com.almoghamdani.quesync",
		"files": [
			"build/**/*",
			"node_modules/**/*"
		],
		"directories": {
			"buildResources": "assets"
		},
		"win": {
			"target": "zip",
			"publisherName": "Almog Hamdani",
			"extraFiles": [
				{
					"from": "backend/build/Release/backend.node",
					"to": "backend.node"
				}
			]
		},
		"mac": {
			"target": "dmg",
			"extraFiles": [
				{
					"from": "backend/build/backend.node",
					"to": "backend.node"
				}
			]
		}
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
