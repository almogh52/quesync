{
  "name": "client",
  "version": "1.0.0",
  "homepage": "./",
  "main": "public/electron.js",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^4.1.5",
    "electron-builder": "^20.40.2",
    "electron-devtools-installer": "^2.2.4",
    "react-scripts": "^2.1.8",
    "redux-logger": "^3.0.6",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "electron-is-dev": "^1.1.0",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "npm": "^6.9.0",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-particles-js": "^2.6.0",
    "react-redux": "^6.0.1",
    "react-transition-group": "^4.0.1",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^6.1.0",
    "rmwc": "^5.3.1",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "react-start": "cross-env SASS_PATH=./node_modules react-scripts start",
    "react-build": "cross-env SASS_PATH=./node_modules react-scripts build",
    "dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "electron .",
    "electron-pack": "build",
    "build": "npm run react-build && npm run electron-pack"
  },
  "build": {
    "appId": "com.almoghamdani.quesync",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraFiles": [
      "backend/bin/**/*"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
